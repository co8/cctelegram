{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://127.0.0.1"
    },
    {
      "pattern": "^https://codesandbox.io/embed"
    },
    {
      "pattern": "^mailto:"
    },
    {
      "pattern": "^tel:"
    },
    {
      "pattern": "^javascript:"
    },
    {
      "pattern": "^#"
    },
    {
      "pattern": "\\.(png|jpg|jpeg|gif|svg|ico|webp)$"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/docs/",
      "replacement": "https://cctelegram.github.io/docs/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
      "headers": {
        "User-Agent": "Mozilla/5.0 (compatible; Link Checker)"
      }
    }
  ],
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206, 301, 302, 303, 307, 308, 403, 405, 503],
  "ignoreDisable": false,
  "quiet": false
}