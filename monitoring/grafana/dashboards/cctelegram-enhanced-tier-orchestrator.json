{
  "dashboard": {
    "id": null,
    "title": "CCTelegram Enhanced Tier Orchestrator",
    "tags": ["cctelegram", "tier-orchestrator", "resilience"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Intelligent Selection Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(cctelegram_intelligent_selection_total[5m]))",
            "legendFormat": "Selections/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "thresholds" },
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 10 },
                { "color": "red", "value": 50 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "Selection Strategy Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (strategy) (increase(cctelegram_selection_strategy_distribution[1h]))",
            "legendFormat": "{{strategy}}"
          }
        ],
        "gridPos": { "h": 8, "w": 6, "x": 6, "y": 0 }
      },
      {
        "id": 3,
        "title": "Error Rate by Tier",
        "type": "stat",
        "targets": [
          {
            "expr": "sum by (tier) (rate(cctelegram_tier_failure_total[5m])) / sum by (tier) (rate(cctelegram_tier_requests_total[5m]))",
            "legendFormat": "{{tier}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "thresholds" },
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 0.01 },
                { "color": "red", "value": 0.05 }
              ]
            },
            "unit": "percentunit"
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 12, "y": 0 }
      },
      {
        "id": 4,
        "title": "Circuit Breaker Status",
        "type": "stat",
        "targets": [
          {
            "expr": "cctelegram_tier_circuit_breaker_state",
            "legendFormat": "{{tier}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              { "options": { "0": { "text": "CLOSED", "color": "green" } } },
              { "options": { "1": { "text": "HALF-OPEN", "color": "yellow" } } },
              { "options": { "2": { "text": "OPEN", "color": "red" } } }
            ]
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 18, "y": 0 }
      },
      {
        "id": 5,
        "title": "Tier Response Times",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum by (tier, le) (rate(cctelegram_tier_response_time_seconds_bucket[5m])))",
            "legendFormat": "{{tier}} p95"
          },
          {
            "expr": "histogram_quantile(0.50, sum by (tier, le) (rate(cctelegram_tier_response_time_seconds_bucket[5m])))",
            "legendFormat": "{{tier}} p50"
          }
        ],
        "yAxes": [
          {
            "unit": "s",
            "min": 0
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 }
      },
      {
        "id": 6,
        "title": "Error Classification Distribution",
        "type": "bargauge",
        "targets": [
          {
            "expr": "sum by (category) (increase(cctelegram_error_classification_total[1h]))",
            "legendFormat": "{{category}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 10 },
                { "color": "red", "value": 100 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 }
      },
      {
        "id": 7,
        "title": "Bulkhead Utilization",
        "type": "graph",
        "targets": [
          {
            "expr": "cctelegram_bulkhead_utilization_ratio",
            "legendFormat": "{{tier}}-{{resource_type}}"
          }
        ],
        "yAxes": [
          {
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        ],
        "thresholds": [
          {
            "value": 0.8,
            "colorMode": "critical",
            "op": "gt"
          }
        ],
        "gridPos": { "h": 8, "w": 8, "x": 0, "y": 16 }
      },
      {
        "id": 8,
        "title": "Priority Queue Depths",
        "type": "graph",
        "targets": [
          {
            "expr": "cctelegram_priority_queue_depth",
            "legendFormat": "{{tier}}-{{priority}}"
          }
        ],
        "yAxes": [
          {
            "min": 0
          }
        ],
        "gridPos": { "h": 8, "w": 8, "x": 8, "y": 16 }
      },
      {
        "id": 9,
        "title": "Self-Healing Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(cctelegram_self_healing_success_total[5m])) / sum(rate(cctelegram_self_healing_attempts_total[5m]))",
            "legendFormat": "Success Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "thresholds" },
            "thresholds": {
              "steps": [
                { "color": "red", "value": null },
                { "color": "yellow", "value": 0.7 },
                { "color": "green", "value": 0.9 }
              ]
            },
            "unit": "percentunit"
          }
        },
        "gridPos": { "h": 8, "w": 8, "x": 16, "y": 16 }
      },
      {
        "id": 10,
        "title": "Adaptive Timeouts",
        "type": "graph",
        "targets": [
          {
            "expr": "cctelegram_adaptive_timeout_seconds",
            "legendFormat": "{{tier}}"
          }
        ],
        "yAxes": [
          {
            "unit": "s",
            "min": 0
          }
        ],
        "gridPos": { "h": 8, "w": 8, "x": 0, "y": 24 }
      },
      {
        "id": 11,
        "title": "Tier Health Scores",
        "type": "stat",
        "targets": [
          {
            "expr": "cctelegram_tier_health_score",
            "legendFormat": "{{tier}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "thresholds" },
            "thresholds": {
              "steps": [
                { "color": "red", "value": null },
                { "color": "yellow", "value": 0.7 },
                { "color": "green", "value": 0.9 }
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "gridPos": { "h": 8, "w": 8, "x": 8, "y": 24 }
      },
      {
        "id": 12,
        "title": "Recovery Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "sum by (tier, strategy) (rate(cctelegram_recovery_attempts_total[5m]))",
            "legendFormat": "{{tier}}-{{strategy}} attempts"
          },
          {
            "expr": "sum by (tier, strategy) (rate(cctelegram_recovery_success_total[5m]))",
            "legendFormat": "{{tier}}-{{strategy}} success"
          }
        ],
        "gridPos": { "h": 8, "w": 8, "x": 16, "y": 24 }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s"
  }
}